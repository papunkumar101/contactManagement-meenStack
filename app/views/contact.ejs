<%- include('includes/_header')%>


<div class="container body-start">
  <div class="row">
   <div class="col-3"></div>
   <div class="col-6">
     <form action="/submit" method="post">
       <div class="mb-3">
         <label for="exampleInputEmail1" class="form-label">Name </label>
         <input type="text" id="name" name="name" class="form-control"  aria-describedby="emailHelp" required> 
       </div>
       <div class="mb-3">
         <label for="exampleInputEmail1" class="form-label">Email </label>
         <input type="email" id="email" name="email" class="form-control"  aria-describedby="emailHelp" required>
         <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
       </div>
       <div class="mb-3">
         <label for="exampleInputPassword1" class="form-label">Phone</label>
         <input type="phone" id="phone" name="phone" class="form-control"  required>
       </div> 
       <div class="float-right">
         <button type="submit" class="btn btn-primary">Save</button>
       </div>
     </form> 
   </div> 
   <div class="col-3"></div>
  </div>
</div>


<%- include('includes/_footer')%>
